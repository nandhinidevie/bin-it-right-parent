{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "installCommand": "chmod +x clone-submodules.sh && ./clone-submodules.sh && curl -fsSL https://fastn.com/install.sh | sh",
  "buildCommand": "chmod +x build.sh && ./build.sh",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/game",
      "destination": "https://bin-it-right.vercel.app/game"
    },
    {
      "source": "/game/:path*",
      "destination": "https://bin-it-right.vercel.app/game/:path*"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "hdi.binitright.com" }],
      "destination": "/hdi/:path*"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "amazon.binitright.com" }],
      "destination": "/amazon/:path*"
    }
  ]
}
