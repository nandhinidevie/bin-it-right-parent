{
  "installCommand": "chmod +x clone-submodules.sh && ./clone-submodules.sh && curl -fsSL -k https://fastn.com/install.sh | sh",
  "buildCommand": "chmod +x build.sh && ./build.sh",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/",
      "has": [
        { "type": "host", "value": "hdi.binitright.com" }
      ],
      "destination": "/hdi"
    }
  ]
}
